{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules" : {
    "no-console": 1,
    "no-undef": 1,
    "no-unused-vars": 1
  }
}
