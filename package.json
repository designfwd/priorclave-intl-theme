{
  "name": "priorclave-international-theme",
  "version": "0.1.0",
  "description": "A theme built for Priorclave International based on Bellhop",
  "main": "index.js",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "scripts": {
    "build": "rimraf {assets/scripts/dist,assets/styles/dist} && webpack",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjatron/priorclave-intl-theme.git"
  },
  "keywords": [
    "wordpress"
  ],
  "author": "Ben Davidson",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/benjatron/priorclave-intl-theme/issues"
  },
  "homepage": "https://github.com/benjatron/priorclave-intl-theme#readme",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.19.0",
    "eslint-loader": "^4.0.2",
    "extend": "^3.0.2",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^6.0.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "normalize-scss": "^7.0.1",
    "postcss-dialog-polyfill": "^1.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.3.0",
    "stylelint": "^13.9.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "bourbon": "^7.0.0",
    "cookieconsent": "^3.1.1",
    "dialog-polyfill": "^0.5.6",
    "flag-icon-css": "^3.5.0",
